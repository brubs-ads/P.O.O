package ListaDeExerc√≠cio2;


public class Questao_1 {
    public static void main(String[] args) {
    
        Integer soma = 0;
       
       for(int i = 1; i<= 500;i++){
            if(i % 2 !=0){
                if(i % 3 == 0){
                    soma+=i;
                }
            }
            }
            
        System.out.println("resultado " + soma);
 }
    }
